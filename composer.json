{
    "name": "dmstr/yii2-libs-metapackage",
    "description": "Core Library Extensions for Phundament 4 (Yii 2.0 Framework)",
    "require": {
        "codemix/yii2-localeurls": "^1.4.5",
        "codemix/yii2-streamlog": "^1.0",
        "dmstr/yii2-bootstrap": "~0.0",
        "dmstr/yii2-db": "~0.7",
        "dmstr/yii2-helpers": "~0.1",
        "dmstr/yii2-migrate-command": "^0.3.2",
        "dmstr/yii2-web": "~0.4",
        "mikehaertl/php-shellcommand": "^1.2.4",
        "schmunk42/retry": "~0.0",
        "yiisoft/yii2-swiftmailer": "2.*"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tobias Munk",
            "email": "tobias@diemeisterei.de"
        }
    ],
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "asset-vcs-driver-options": {
            "github-no-api": true
        },
        "asset-pattern-skip-version": "(-build|-patch)"
    }
